 Make max:bot turn right at a set speed" line44="     */" line45="    //% blockID=maxbotRightSpeed" line46="    //% block=&quot;Max:Bot turn right at $speed&quot;" line47="    //% speed.min=1 speed.max=90" line48="    export function turnRight(speed: number) : void {" line49="        speed |= 0;" line50="        pins.servoWritePin(leftMotor, 90 + speed);" line51="        pins.servoWritePin(rightMotor, 90 + speed);" line52="    }" line53="" line54="    /**" line55="     * Set max:bot left wheel speed" line56="     */" line57="    //% blockID=maxbotLeftWheelSpeed" line58="    //% block=&quot;Max:Bot set left wheel to $speed&quot;" line59="    //% speed.min=-90 speed.max=90" line60="    export function setLeftWheelSpeed(speed:number) : void {" line61="        speed |= 0;" line62="        pins.servoWritePin(leftMotor, speed);" line63="    }" line64="    " line65="    /**" line66="     * Set max:bot right wheel speed" line67="     */" line68="    //% blockID=maxbotRightWheelSpeed" line69="    //% block=&quot;Max:Bot set right wheel to $speed&quot;" line70="    //% speed.min=-90 speed.max=90" line71="    export function setRightWheelSpeed(speed: number): void {" line72="        speed |= 0;" line73="        pins.servoWritePin(rightMotor, speed);" line74="    }" line75="" line76="    /**" line77="     * Get left strip on max:bot" line78="     */" line79="    //% blockID=maxbotGetLeftStrip" line80="    //% block=&quot;Max:Bot left strip&quot;" line81="    export function getLeftStrip() : neopixel.Strip{" line82="        return leftStrip;" line83="    }" line84="    " line85="    /**" line86="     * Get right strip on max:bot" line87="     */" line88="    //% blockID=maxbotGetRightStrip" line89="    //% block=&quot;Max:Bot right strip&quot;" line90="    export function getRightStrip(): neopixel.Strip {" line91="        return rightStrip;" line92="    }" line93="}" numlines="94"></mutation></block></statement></block></xml>